{
  "_default_": {
    "show_unicode_error": false,
    "sort_files": true,
    "relative_path": true,
    "path_only": false,
    "context_lines": 1,
    "with_suffix": [],
    "no_suffix": [
      ".png",
      ".jpg",
      ".bmp",
      ".ttf",
      ".svg",
      ".ico",
      ".lock",
      ".orig"
    ],
    "no_start_dir": [".", "__pycache__", "build", ".venv"],
    "no_start_file": [".", "~"]
  },
  "all": {
    "no_start_dir": ["__pycache__"]
  },
  "py": {
    "with_suffix": [".py", ".pxi"],
    "no_start_dir": [".", "__pycache__", "tests", "dist", "build"]
  },
  "pyt": {
    "with_suffix": [".py", ".pxi"],
    "no_start_dir": [".", "__pycache__", "dist", "build"]
  },
  "py0": {
    "with_suffix": [".py", ".pxi"],
    "no_start_dir": [".", "__pycache__", "tests", "dist", "build"],
    "context_lines": 0
  },
  "py2": {
    "with_suffix": [".py", ".pxi"],
    "no_start_dir": [".", "__pycache__", "tests", "dist", "build"],
    "context_lines": 2
  },
  "nopy": {
    "no_start_dir": [".", "__pycache__", "tests", "dist", "build"],
    "no_suffix": [
      ".py",
      ".png",
      ".jpg",
      ".bmp",
      ".ttf",
      ".svg",
      ".ico",
      ".orig"
    ]
  },
  "md": {
    "with_suffix": [".md"]
  },
  "txt": {
    "with_suffix": [".txt"]
  },
  "cfg": {
    "with_suffix": [".cfg", ".ini"]
  },
  "rb": {
    "with_suffix": [".rb"]
  },
  "rb0": {
    "with_suffix": [".rb"],
    "context_lines": 0
  },
  "js": {
    "with_suffix": [".js", ".ts", ".vue"]
  },
  "css": {
    "with_suffix": [".css"]
  },
  "json": {
    "with_suffix": [".json"]
  },
  "toml": {
    "with_suffix": [".toml"]
  },
  "html": {
    "with_suffix": [".html", ".htm", ".j2"]
  },
  "htmlall": {
    "with_suffix": [".html", ".htm", ".j2", ".js", ".ts", ".vue", ".css"]
  },
  "yml": {
    "context_lines": 2,
    "with_suffix": [".yml", ".yaml"]
  },
  "sh": {
    "with_suffix": [".sh"]
  }
}
