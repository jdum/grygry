{
  "_default_": {
    "show_unicode_error": false,
    "sort_files": true,
    "relative_path": true,
    "path_only": false,
    "context_lines": 1,
    "with_suffix": [],
    "no_suffix": [
      ".png",
      ".jpg",
      ".bmp",
      ".ttf",
      ".svg",
      ".ico",
      ".lock",
      ".orig"
    ],
    "no_start_dir": [".", "__pycache__"],
    "no_start_file": [".", "~"]
  },
  "all": {
    "no_start_dir": ["__pycache__"]
  },
  "all00": {
    "no_start_dir": ["__pycache__"],
    "path_only": true
  },
  "py": {
    "with_suffix": [".py", ".pxi"],
    "no_start_dir": [".", "__pycache__", "tests"]
  },
  "pyt": {
    "with_suffix": [".py", ".pxi"],
    "no_start_dir": [".", "__pycache__"]
  },
  "py00": {
    "with_suffix": [".py", ".pxi"],
    "no_start_dir": [".", "__pycache__", "tests"],
    "path_only": true
  },
  "py0": {
    "with_suffix": [".py", ".pxi"],
    "no_start_dir": [".", "__pycache__", "tests"],
    "context_lines": 0
  },
  "py2": {
    "with_suffix": [".py", ".pxi"],
    "no_start_dir": [".", "__pycache__", "tests"],
    "context_lines": 2
  },
  "nopy": {
    "no_start_dir": [".", "__pycache__", "tests"],
    "no_suffix": [
      ".py",
      ".png",
      ".jpg",
      ".bmp",
      ".ttf",
      ".svg",
      ".ico",
      ".orig"
    ]
  },
  "md": {
    "with_suffix": [".md", ".MD"]
  },
  "txt": {
    "with_suffix": [".txt", ".TXT"]
  },
  "cfg": {
    "with_suffix": [".cfg", ".ini", ".INI", ".CFG"]
  },
  "rb": {
    "with_suffix": [".rb"]
  },
  "rb00": {
    "with_suffix": [".rb"],
    "path_only": true
  },
  "rb0": {
    "with_suffix": [".rb"],
    "context_lines": 0
  },
  "js": {
    "with_suffix": [".js", ".JS", ".ts", ".vue"]
  },
  "css": {
    "with_suffix": [".css", ".CSS"]
  },
  "json": {
    "with_suffix": [".json", ".JSON"]
  },
  "toml": {
    "with_suffix": [".toml", ".TOML"]
  },
  "html": {
    "with_suffix": [".html", ".htm", ".HTML", ".HTM"]
  },
  "html00": {
    "with_suffix": [".html", ".htm", ".HTML", ".HTM"],
    "path_only": true
  },
  "htmlall": {
    "with_suffix": [
      ".html",
      ".htm",
      ".js",
      ".ts",
      ".vue",
      ".css",
      ".HTML",
      ".HTM",
      ".JS",
      ".CSS"
    ]
  },
  "yml": {
    "context_lines": 2,
    "with_suffix": [".yml", ".yaml", ".YML", ".YAML"]
  },
  "sh": {
    "with_suffix": [".sh"]
  }
}
