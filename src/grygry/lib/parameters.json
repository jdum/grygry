{
  "_default_": {
    "show_unicode_error": false,
    "sort_files": true,
    "relative_path": true,
    "path_only": false,
    "context_lines": 1,
    "with_suffix": [],
    "no_suffix": [
      ".bmp",
      ".doc",
      ".docx",
      ".dump",
      ".gz",
      ".ico",
      ".jpg",
      ".lock",
      ".odp",
      ".ods",
      ".odt",
      ".orig",
      ".pdf",
      ".png",
      ".sql",
      ".svg",
      ".tar",
      ".tgz",
      ".ttf",
      ".xls",
      ".xlsx",
      ".zip"
    ],
    "no_dir": ["__pycache__", "build", "dist", "htmlcov", "tmp"],
    "no_start_dir": ["."],
    "no_start_file": [".", "~"]
  },
  "all": {},
  "py": {
    "with_suffix": [".py", ".pxi"],
    "no_dir": ["__pycache__", "build", "dist", "htmlcov", "tests", "tmp"],
    "no_start_dir": ["."]
  },
  "pyt": {
    "with_suffix": [".py", ".pxi"],
    "no_dir": ["__pycache__", "build", "dist", "htmlcov", "tmp"],
    "no_start_dir": ["."]
  },
  "py0": {
    "with_suffix": [".py", ".pxi"],
    "no_dir": ["__pycache__", "build", "dist", "htmlcov", "tests", "tmp"],
    "no_start_dir": ["."],
    "context_lines": 0
  },
  "py2": {
    "with_suffix": [".py", ".pxi"],
    "no_dir": ["__pycache__", "build", "dist", "htmlcov", "tests", "tmp"],
    "no_start_dir": ["."],
    "context_lines": 2
  },
  "nopy": {
    "no_dir": ["__pycache__", "build", "dist", "htmlcov", "tests", "tmp"],
    "no_start_dir": ["."],
    "no_suffix": [
      ".py",
      ".bck",
      ".bmp",
      ".doc",
      ".docx",
      ".dump",
      ".gz",
      ".ico",
      ".jpg",
      ".lock",
      ".odp",
      ".ods",
      ".odt",
      ".orig",
      ".pdf",
      ".png",
      ".sql",
      ".svg",
      ".tar",
      ".tgz",
      ".ttf",
      ".xls",
      ".xlsx",
      ".zip"
    ]
  },
  "md": {
    "with_suffix": [".md"]
  },
  "txt": {
    "with_suffix": [".txt"]
  },
  "cfg": {
    "with_suffix": [".cfg", ".ini"]
  },
  "rb": {
    "with_suffix": [".rb"]
  },
  "rb0": {
    "with_suffix": [".rb"],
    "context_lines": 0
  },
  "js": {
    "with_suffix": [".js", ".ts", ".vue"]
  },
  "css": {
    "with_suffix": [".css"]
  },
  "json": {
    "with_suffix": [".json"]
  },
  "toml": {
    "with_suffix": [".toml"]
  },
  "html": {
    "with_suffix": [".html", ".htm", ".j2"]
  },
  "htmlall": {
    "with_suffix": [".html", ".htm", ".j2", ".js", ".ts", ".vue", ".css"]
  },
  "yml": {
    "context_lines": 2,
    "with_suffix": [".yml", ".yaml"]
  },
  "sh": {
    "with_suffix": [".sh"]
  }
}
